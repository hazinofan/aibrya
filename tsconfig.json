{
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "preserve",
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["nuxt", "@primevue/nuxt-module", "node"],
    "baseUrl": ".",
    "paths": {
      "#app": ["./.nuxt/types/app"],
      "#build": ["./.nuxt/types/build"],
      "#imports": ["./.nuxt/types/imports"]
    }
  },
  "include": ["**/*.ts", "**/*.vue", "app/types", ".nuxt"]
}
